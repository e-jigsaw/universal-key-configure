manifest_version: 2
name: 'universal-key-configure'
description: 'Universal key configure'
version: '0.0.1'
content_security_policy: "script-src 'self' 'unsafe-eval'; object-src 'self'"
permissions: [
  'tabs'
]
options_page: 'option.html'
content_scripts: [
  matches: [
    'http://*/*'
    'https://*/*'
  ]
  js: ['index.js']
]
